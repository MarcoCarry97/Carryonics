<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Insert</servlet-name>
        <servlet-class>operations.Insert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpDelProduct</servlet-name>
        <servlet-class>operations.UpDelProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddSong</servlet-name>
        <servlet-class>operations.AddSong</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpDelSong</servlet-name>
        <servlet-class>operations.UpDelSong</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveSong</servlet-name>
        <servlet-class>operations.RemoveSong</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddActor</servlet-name>
        <servlet-class>operations.AddActor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpDelActor</servlet-name>
        <servlet-class>operations.UpDelActor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveActor</servlet-name>
        <servlet-class>operations.RemoveActor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Insert</servlet-name>
        <url-pattern>/Insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpDelProduct</servlet-name>
        <url-pattern>/UpDelProduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddSong</servlet-name>
        <url-pattern>/AddSong</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpDelSong</servlet-name>
        <url-pattern>/UpDelSong</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveSong</servlet-name>
        <url-pattern>/RemoveSong</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddActor</servlet-name>
        <url-pattern>/AddActor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpDelActor</servlet-name>
        <url-pattern>/UpDelActor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveActor</servlet-name>
        <url-pattern>/RemoveActor</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
